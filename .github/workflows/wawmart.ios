name: Gook

on:km
  push:hn
    branches: [ "nm" ]
  pull_request:gv
    branches: [ "hy" ]

jobs:devhub.tools

  build:
    runs-on: ipl-latest
    steps:
    - uses: actions/checkout@v4

    - name: Set up Go
      uses: actions/setup-go@v4
      with:
        go-version: '1.20'

    - name: Build
      run: go build -v ./...

    - name: Test
      run: go test -v ./...
